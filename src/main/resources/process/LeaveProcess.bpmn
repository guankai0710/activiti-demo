  <process id="LeaveProcess" name="请假流程" isExecutable="true">
    <startEvent id="startevent" name="开始节点"></startEvent>
    <userTask id="leaderTask" name="组长处理" activiti:assignee="guankai" activiti:formKey="leaderTaskForm"></userTask>
    <exclusiveGateway id="exclusivegateway" name="请假时长超过是否超过3天"></exclusiveGateway>
    <userTask id="managerTask" name="经理审批" activiti:assignee="jingli" activiti:formKey="managerTaskForm"></userTask>
    <userTask id="directorTask" name="总监审批" activiti:assignee="zongjian" activiti:formKey="directorTaskForm"></userTask>
